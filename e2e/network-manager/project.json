{
  "sourceRoot": "e2e/network-manager/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "e2e/network-manager/dist",
        "tsConfig": "e2e/network-manager/tsconfig.json",
        "main": "e2e/network-manager/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "e2e/network-manager/**/*.{ts,js}"
        ]
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "e2e/network-manager/out",
        "configFile": "e2e/network-manager/vite.config.ts"
      }
    },
    "e2e": {
      "executor": "@dxos/playwright:playwright",
      "options": {
        "testPatterns": [
          "e2e/network-manager/playwright/tests/*.spec.ts"
        ],
        "config": "e2e/network-manager/playwright/config.ts"
      }
    }
  },
  "implicitDependencies": [
    "playwright"
  ]
}
