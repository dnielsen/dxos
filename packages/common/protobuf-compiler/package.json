{
  "name": "@dxos/protobuf-compiler",
  "version": "2.33.8",
  "license": "MIT",
  "author": "DXOS.org",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "build-protobuf": "./bin/main.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm run gen && tsc",
    "check": "pnpm run build && pnpm run lint && pnpm run test",
    "gen": "ts-node src/main.ts test/proto/schema.proto test/proto/another.proto -s test/proto/substitutions.ts -o test/proto/gen",
    "lint": "eslint 'src/**/*.ts'",
    "test": "mocha -r @swc-node/register '**/*.test.ts'"
  },
  "publishConfig": {
    "access": "public"
  },
  "executors": "./executors.json"
}
