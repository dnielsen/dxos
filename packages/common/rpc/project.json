{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/common/rpc/src",
  "projectType": "library",
  "targets": {
    "proto": {
      "executor": "@dxos/protobuf-compiler:generate",
      "outputs": [
        "{options.basePath}{options.outputPath}"
      ],
      "options": {
        "basePath": "packages/common/rpc/src/proto/",
        "srcPath": "**/*.proto",
        "outputPath": "gen",
        "substitutionsPath": "substitutions.ts"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/common/rpc/dist",
        "main": "packages/common/rpc/src/index.ts",
        "tsConfig": "packages/common/rpc/tsconfig.json",
        "packageJson": "packages/common/rpc/package.json",
        "assets": ["packages/common/rpc/*.md"]
      }
    }
  }
}
