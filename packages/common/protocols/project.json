{
  "sourceRoot": "packages/common/protocols/src",
  "projectType": "library",
  "targets": {
    "proto": {
      "executor": "@dxos/protobuf-compiler:generate",
      "outputs": [
        "{options.basePath}{options.outputPath}"
      ],
      "options": {
        "basePath": "packages/common/protocols/src/proto/",
        "srcPath": "**/*.proto",
        "outputPath": "gen",
        "substitutionsPath": "substitutions.ts"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/common/protocols/dist",
        "main": "packages/common/protocols/src/index.ts",
        "tsConfig": "packages/common/protocols/tsconfig.json",
        "packageJson": "packages/common/protocols/package.json",
        "assets": ["packages/common/protocols/*.md"]
      }
    }
  },
  "implicitDependencies": [
    "codec-protobuf"
  ]
}
