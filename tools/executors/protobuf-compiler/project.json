{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/executors/protobuf-compiler/src",
  "projectType": "library",
  "targets": {
    "prebuild": {
      "executor": "nx:run-script",
      "outputs": [
        "tools/executors/protobuf-compiler/test/proto/gen"
      ],
      "options": {
        "script": "prebuild"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "tools/executors/protobuf-compiler/dist",
        "tsConfig": "tools/executors/protobuf-compiler/tsconfig.json",
        "main": "tools/executors/protobuf-compiler/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "tools/executors/protobuf-compiler/**/*.{ts,js}"
        ]
      }
    },
    "test": {
      "executor": "@dxos/mocha:mocha",
      "outputs": [
        "{options.outputPath}",
        "{options.resultsPath}",
        "{options.coveragePath}"
      ],
      "options": {
        "testPatterns": [
          "tools/executors/protobuf-compiler/src/**/*.test.{ts,js}"
        ],
        "watchPatterns": [
          "tools/executors/protobuf-compiler/src/**/*"
        ],
        "outputPath": "tmp/mocha/tools/executors/protobuf-compiler",
        "resultsPath": "test-results/tools/executors/protobuf-compiler",
        "coveragePath": "coverage/tools/executors/protobuf-compiler"
      }
    }
  }
}
