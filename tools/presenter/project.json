{
  "sourceRoot": "tools/presenter/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "tools/presenter/src/index.ts",
        "outputPath": "tools/presenter/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "tools/presenter/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "bundle": {
      "executor": "@nxext/vite:build",
      "inputs": [
        "^production",
        "production"
      ],
      "options": {
        "configFile": "tools/presenter/vite.config.ts",
        "outputPath": "tools/presenter/out/presenter"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    }
  }
}
